{
	"manifest_version":2,
	"name":"TabMarker",
	"version":"1.0.0",
	"description":"Saves and organizes tabs.",
	"author":"Yo",
	"background":{
		"scripts":["background.js"],
		"persistent":false
	},
	"options_page":"options.html",
	"permissions":["tabs","storage"],
	"incognito":"split",
	"browser_action":{
		"title":"Save and organize tabs"
	}
}