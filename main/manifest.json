{
	"manifest_version": 3,
	"name": "TabMarker",
	"version": "0.1.0",
	"description": "Saves and organizes tabs.",
	"author": "Yo",
	"background": {
	   "service_worker": "background.js"
	},
	"options_page": "view/options.html",
	"permissions": [
		"tabs",
		"storage",
		"scripting"
	],
	"incognito": "split",
	"action": {
		"title": "Save and organize tabs"
	}
}